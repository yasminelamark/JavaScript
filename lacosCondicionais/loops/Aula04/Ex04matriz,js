const leia = require('readline-syc');

let matriz = new Array(2);

for(let indice = 0; indice < matriz.length; indice ++){
    matriz[indice] = Array(4);
}

let media = [];
let soma = 0.0
let nNotas = 0;

for (let idiceLinha = 0; indiceLinha < matriz.length; indiceLinha ++ ){

    for (let indiceColuna = 0; indiceColuna < matriz[indiceLinha].lenght;indiceColuna ++){

        matriz[idiceLinha][indiceColuna] = leia.questionInt 
    }
}

